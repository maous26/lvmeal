{
  "expo": {
    "name": "Presence",
    "slug": "presence",
    "owner": "maous1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "presence",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.presence.nutrition",
      "infoPlist": {
        "NSCameraUsageDescription": "Presence utilise la caméra pour scanner les aliments et codes-barres.",
        "NSPhotoLibraryUsageDescription": "Presence accède à vos photos pour analyser les repas."
      }
    },
    "android": {
      "package": "com.presence.nutrition",
      "versionCode": 1,
      "minSdkVersion": 26,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "VIBRATE",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      "expo-camera",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#0077B6"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d22ac2c1-1cd1-4437-aa28-3c1c7ebfdef9"
      }
    }
  }
}
