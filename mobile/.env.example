# ============================================
# LYM Mobile App - Environment Variables
# ============================================
# Copy this file to .env and fill in your values

# OpenAI API Key for LymIA features
EXPO_PUBLIC_OPENAI_API_KEY=sk-...

# RapidAPI - Gustar.io for recipes
EXPO_PUBLIC_RAPIDAPI_KEY=

# Google AI (optional)
GOOGLE_AI_API_KEY=
GOOGLE_AI_MODEL=gemini-1.5-pro

# Supabase (RAG, chat history, knowledge base, cloud sync)
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Google OAuth (for cloud sync authentication)
# Get these from https://console.cloud.google.com/
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=

# Cloudflare R2 Storage (photos)
EXPO_PUBLIC_R2_ACCOUNT_ID=
EXPO_PUBLIC_R2_ACCESS_KEY_ID=
EXPO_PUBLIC_R2_SECRET_ACCESS_KEY=
EXPO_PUBLIC_R2_BUCKET_NAME=lym-photos
EXPO_PUBLIC_R2_PUBLIC_URL=https://your-bucket.your-domain.com
